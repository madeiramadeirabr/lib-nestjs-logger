{
  "name": "nest-js-logger",
  "version": "1.0.0",
  "description": "Nestjs library to provide the easy and maintainable  mode to following mmrfc convention",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --config=jest-config.json",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeiramadeirabr/nest-js-logger.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@types/jest": "29.0.2",
    "@types/node": "18.7.18",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madeiramadeirabr/nest-js-logger/issues"
  },
  "homepage": "https://github.com/madeiramadeirabr/nest-js-logger#readme",
  "dependencies": {
    "@newrelic/winston-enricher": "^4.0.0",
    "nest-winston": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.8.2"
  }
}
