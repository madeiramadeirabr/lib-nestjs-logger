{
    "moduleFileExtensions": ["ts", "tsx", "js", "json"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/tests/.*\\.(test|spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**"
    ],
    "coverageReporters": ["json", "lcov"]
  }